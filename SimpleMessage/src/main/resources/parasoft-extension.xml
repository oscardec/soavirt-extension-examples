<?xml version="1.0" encoding="UTF-8"?>
<extension xmlns="http://schemas.parasoft.com/extensibility-framework/v1/extension"
 type="messageFormat"
 name="SimpleMessage"
 description="A simple message example provided to demonstrate how to create Custom Message Formats in SOAtest">
    <class>com.parasoft.soavirt.messages.simplemessage.SimpleMessageConverter</class>
    <messageFormat xmlns="http://schemas.parasoft.com/extensibility-framework/messageFormat">
        <defaultMimetype>text/plain</defaultMimetype>
        <messageTypes>
        <messageType id="SimpleSchema" name="SimepleSchema" xsd="SimpleSchema.xsd"/>
        <messageType id="NameSchema" name="AnotherSimepleSchema" xsd="NameSchema.xsd"/>
        </messageTypes>
    </messageFormat>
</extension>