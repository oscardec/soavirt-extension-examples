<?xml version="1.0" encoding="UTF-8"?>
<extension xmlns="http://schemas.parasoft.com/extensibility-framework/v1/extension"
 type="transport"
 name="Simple HTTP Transport"
 description="An extensible transport provided to SOAtest via the Extensibility Framework">
    <class>com.parasoft.soavirt.transport.http.SimpleHTTPTransport</class>
    <version id="3" updaterClass="com.parasoft.soavirt.transport.http.VersionUpdater" />
    <form xmlns="urn:com/parasoft/extensibility-framework/gui">
        <section label="Connection">
            <field id="URL" label="URL" />
        </section>
        <section label="Headers">
            <field id="HeaderName" label="Header Name" />
            <field id="HeaderValue" label="Header Value" />
        </section>
    </form>
</extension>
